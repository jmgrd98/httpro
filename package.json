{
  "name": "api-client-v2",
  "version": "0.1.0",
  "private": true,
  "main": "main/main.js",
  "author": "RBFraphael",
  "description": "Electron + NextJS example project",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron . --no-sandbox\"",
    "build": "next build && electron-builder --win --mac --linux --publish always",
    "build:all": "next build && electron-builder --win --mac --linux --publish always",
    "build:win": "next build && electron-builder --win --publish always",
    "build:mac": "next build && electron-builder --mac --publish always",
    "build:linux": "next build && electron-builder --linux --publish always",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.10",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@uiw/codemirror-theme-tokyo-night": "^4.21.25",
    "@uiw/react-codemirror": "^4.21.25",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "electron-serve": "^2.1.1",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.359.0",
    "next": "^14.2.25",
    "prisma": "^5.22.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-toastify": "^10.0.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.1.2",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
